{
	"trailingComma": "es5",
	"semi": true,
	"htmlWhitespaceSensitivity": "strict",
	"tabWidth": 4,
	"useTabs": true,
	"singleQuote": true,
	"endOfLine": "lf",
	"printWidth": 120,
	"overrides": [
		{
			"files": ["*.yml", "*.yaml", "package.json"],
			"options": {
				"tabWidth": 2,
				"useTabs": false
			}
		},
		{
			"files": ["*.md"],
			"options": {
				"tabWidth": 4,
				"useTabs": false
			}
		}
	]
}
